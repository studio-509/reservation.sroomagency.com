#calendar_table {
	border-collapse:separate;
	border-color: #fff;
	width:100%;
}
.tableau { display:table;}
#calendar_table .tr {
	width:100%;
	clear:both;
	border-bottom:1px solid #ccc;
	display:table-row;
}
#calendar_table .th,
#calendar_table .td {
	float:left;
	width:100%;
	padding:0px;
	display:table-cell;
	border:1px solid #fff;
}
#calendar_table .th {
	background:#333;
	color:#fff;
	border:1px solid #FFF;
	padding:10px;
	height:70px;
}
#calendar_table .th .date {
	font-weight:bold;
	font-size:0.8em;}
#calendar_table a { display:block; padding:8px; color:#fff;}
#calendar_table a._admin_resa {color : #000;}
#calendar_table .tr .td.horaires {
	background:#eee;}
#calendar_table .tr .td.reserve{
	background-color: rgba(251, 0, 0, 0.66);
}
#calendar_table .tr .td.reserve a {
	cursor:default;
}
#calendar_table .tr .td.libre,
#reservation_table .tr.libre {
	background-color:#336600;
	color:#fff;
	transition:all ease 0.6s;
}
#calendar_table .tr .td.libre:hover {
	background-color:#669933;
	transition:all ease 0.6s;
}
#calendar_table .tr .td.passe,
#reservation_table .tr.passe{
	
	background-color:rgba(106, 100, 100, 0.66);
	color:#333;
}
#calendar_table .tr .td.passe a,
#reservation_table .tr.passe a{

	cursor:default;
}
#calendar_table .tr .td.encours,
#reservation_table .tr.encours{
	background-color:rgba(235, 58, 2, 0.66);
	color:#fff;
}
#calendar_table .tr .td.encours a,
#reservation_table .tr.encours a{

	cursor:default;
}
.resa_alerte, .bg_error{
	background-color:rgba(255, 0, 0, 0.66)
}
@media all and (max-width: 767px) {
	#calendar_table .th,
	#calendar_table .tr .td { width:50%; display:block; float:left; overflow:hidden;}
	#calendar_table .tr .td.col01 { width:100%;}
	#calendar_table .tr { clear:both;}
}
@media all and (min-width: 768px) {
	#calendar_table .th,
	#calendar_table .td { width:14.28%;}
}
